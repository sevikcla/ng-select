{
  "$schema": "../node_modules/ng-packagr/package.schema.json",
  "name": "@sevikcla/ng-select",
  "version": "1.0.5",
  "description": "Fork of Angular ng-select - All in One UI Select, Multiselect and Autocomplete",
  "author": "sevikcla",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sevikcla/ng-select.git"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "keywords": [
    "angular",
    "select",
    "ui-select",
    "dropdown",
    "multiselect",
    "autocomplete",
    "angular2",
    "angular4",
    "netlang"
  ],
  "peerDependencies": {
    "@angular/common": ">=6.0.0 <7.0.0",
    "@angular/core": ">=6.0.0 <7.0.0",
    "@angular/forms": ">=6.0.0 <7.0.0"
  },
  "ngPackage": {
    "lib": {
      "flatModuleFile": "ng-select",
      "entryFile": "index.ts"
    },
    "dest": "../dist"
  }
}
